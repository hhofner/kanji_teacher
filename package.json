{
  "name": "kanji-teacher",
  "version": "0.1.5",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "run-s build:*",
    "start": "remix-serve ./build/index.js",
    "typecheck": "tsc",
    "db:migrations": "drizzle-kit generate:sqlite --out ./app/drizzle/migrations --schema ./app/drizzle/schema.server.ts",
    "db:drop": "drizzle-kit drop --config=./app/drizzle/config.server.ts",
    "db:studio": "drizzle-kit studio --config=./app/drizzle/config.server.ts",
    "format": "prettier . --write",
    "build:remix": "remix build",
    "build:worker": "remix-pwa build",
    "dev": "run-p dev:*",
    "dev:remix": "remix dev --manual",
    "dev:worker": "remix-pwa dev"
  },
  "dependencies": {
    "@libsql/client": "^0.4.0-pre.5",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@remix-pwa/cache": "^2.0.12",
    "@remix-pwa/client": "^2.0.1",
    "@remix-pwa/push": "^1.0.9",
    "@remix-pwa/strategy": "^2.1.9",
    "@remix-pwa/sw": "^2.1.12",
    "@remix-pwa/sync": "^2.0.1",
    "@remix-run/css-bundle": "^2.3.0",
    "@remix-run/node": "^2.3.0",
    "@remix-run/react": "^2.3.0",
    "@remix-run/serve": "^2.3.0",
    "@types/bcrypt": "^5.0.2",
    "@types/better-sqlite3": "^7.6.7",
    "@types/lazy-brush": "^1.0.3",
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^9.1.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.29.0",
    "isbot": "^3.6.8",
    "jmdict-simplified-node": "^1.1.2",
    "lazy-brush": "^2.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-utils": "^7.1.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.8.9",
    "wanakana": "^5.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.5.0",
    "@remix-pwa/dev": "2.0.31",
    "@remix-pwa/worker-runtime": "^2.0.8",
    "@remix-run/dev": "^2.3.0",
    "@remix-run/eslint-config": "^2.3.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8.38.0",
    "glob": "^10.3.10",
    "npm-run-all": "^4.1.5",
    "prettier": "3.1.1",
    "remix-pwa": "^3.0.19",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
